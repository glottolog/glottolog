@article{101365,
    author = {Guillaume, Antoine},
    title = {Hierarchical Agreement and Split Intransitivity in Reyesano},
    journal = {International Journal of American Linguistics},
    volume = {75},
    number = {1},
    pages = {29–48},
    year = {2009},
    issn = {0020-7071},
    url = {http://www.jstor.org/stable/10.1086/598202},
    abstract = {This paper discusses, for the first time, the peculiar system of person marking in Reyesano, an Amazonian language from northern Bolivia. Person marking in Reyesano is quite unusual. First, it is discontinuous, involving a prefix slot for the indexation of first and second persons and a suffix slot for the indexation of third persons. Second, the use of the person markers in the slots is conditioned by the complex interaction of distinct factors, involving the grammatical function of the participants (S, A, and O), their ranking on a person hierarchy (2 > 1 > 3), and their number (singular vs. plural).},
    doi = {10.1086/598202},
    glottolog_ref_id = {101365},
    inlg = {English [eng]},
    keywords = {Agreement, Bolivia, South America, Syntax, Tacanan, Typology},
    languageid = {2997},
    lgcode = {Reyesano [reye1240]},
    macro_area = {South America},
    src = {autotyp, gj, haspelmath, seifart}
}
