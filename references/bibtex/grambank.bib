@mastersthesis{g_Dillon_Tatana,
    author = {Dillon, John Albert},
    title = {A Grammatical Description of Tatana' (Malaysia)},
    school = {University of Texas at Arlington},
    pages = {167},
    year = {1994},
    abstract = {Tatana' is a Western Austronesian language in the southwest corner of the state of Sabah, Malaysia. This thesis is an initial description of the grammar of the language. The scope of the grammar is broad, encompassing several aspects of the grammar, rather than deep in any one aspect. It is the aim of this broad grammar to be a foundation on which to build further research. The most interesting aspects of the language involve the verbal construction. The Tatana' language is characteristic of other Philippine-type languages in that it has a complex voice system, traditionally called a focus system. Six voices are listed, though a case is made for reducing that number to four, based on the analysis of affectedness within the language. Affectedness is one of the more notable features discussed and is pertinent in the analysis of several areas of the grammar. The study of non-volitive mood in Tatana' yields information about the verb in terms of intentionality and whether or not the reported event actually took place.},
    adviser = {Edmondson, Jerold A.},
