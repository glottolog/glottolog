@article{newton_radio_2013,
    author = {Newton, Gerald},
    title = {Radio Luxembourg in Peace and War},
    journal = {German Life and Letters},
    volume = {66},
    number = {1},
    pages = {55--76},
    year = {2013},
    issn = {1468-0483},
    url = {http://onlinelibrary.wiley.com/doi/10.1111/glal.12003/abstract},
    abstract = {The article traces the development of commercial broadcasting in the 1930s from Radio Luxembourg, the high-power commercial station located in Luxembourg and supposedly biased towards French propaganda, depicting how both the Third Reich and the BBC treated it with hostility. When on 8 May 1940 Luxembourg, a neutral country since 1867, fell into the hands of Nazi Germany, the transmitter became part of the German Reichssender network, and remained so until Liberation in September 1944, whereafter it was taken over as a United Nations transmitter under control of SHAEF, until finally being returned to the Radio Luxembourg Company on 12 November 1945, and once more transmitted, as before the war, programmes in Luxembourgish, French, English and German. These, with the exception of the English programmes, which ended on 31 December 1992, continue to this day.Der Artikel verfolgt die Entwicklung des privaten, doch angeblich propagandistisch Frankreich gesinnten Großsenders Radio-Luxemburg in den 1930er Jahren und veranschaulicht, wie ihm sowohl das Dritte Reich als auch die britische Rundfunkgesellschaft BBC feindlich gegenüberstanden. Als am 8. Mai 1940 das seit 1867 neutralisierte Luxemburg Nazideutschland zum Opfer fiel, wurde der Sender bis zur Befreiung Luxemburgs im September 1944 an das deutsche Reichssendernetz angeschlossen, wonach er ein Sender der Vereinigten Nationen unter Führung des Ober-Kommandos der Alliierten Streitkräfte in Europa wurde, bis er schließlich am 12. November 1945 zur Radio-Luxemburg-Gesellschaft zurückkommen konnte und wie vor dem Krieg luxemburgisch-, französisch-, englisch- und deutschsprachige Programme wieder ausstrahlen durfte, was noch heute mit Ausnahme der englischen, die am 31. Dezember 1992 endeten, der Fall ist.},
    copyright = {© The author 2013. German Life and Letters © Blackwell Publishing Ltd. 2013},
    doi = {10.1111/glal.12003},
    glottolog_ref_id = {471734},
    inlg = {English [eng]},
    lgcode = {Luxembourgish [ltz]},
    urldate = {2013-02-05}
}
