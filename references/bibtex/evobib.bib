@article{GuevaraErra2016,
    author = {Guevara Erra, Ramon AND Gervain, Judit},
    title = {The Efficient Coding of Speech: Cross-Linguistic Differences},
    journal = {PLoS ONE},
    publisher = {Public Library of Science},
    volume = {11},
    number = {2},
