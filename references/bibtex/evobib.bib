@article{Jaeger2018a,
    author = {Gerhard Jäger and List, Johann-Mattis},
    title = {{U}sing ancestral state reconstruction methods for onomasiological reconstruction in multilingual word lists},
    journal = {Language Dynamics and Change},
    volume = {8},
